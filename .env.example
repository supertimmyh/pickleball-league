# Google Cloud Configuration
# Set to 'true' to enable Google Cloud Storage backend
USE_GCS=false

# Your Google Cloud Project ID
GOOGLE_CLOUD_PROJECT=your-project-id

# Cloud Storage bucket names
GCS_MATCHES_BUCKET=pickleball-matches-data
GCS_CONFIG_BUCKET=pickleball-config-data

# Flask Configuration
FLASK_ENV=production

# Cloud Run Port (automatically set by Cloud Run, but can be overridden locally)
PORT=8000
